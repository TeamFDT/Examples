package {	import flash.events.Event;	import flash.display.MovieClip;	import ${packageStructure}.modules.contexts.AppContext;	/**	* @class MainApplication	* @author Dave Gamez	* Copyright (c) 2010 dave@swfgeek.net	* Released under MIT license: 	* http://www.opensource.org/licenses/mit-license.php	* @date June 21 2011, 6:00:00 AM	* @version 0.2	*/	public class  MainApplication extends MovieClip {		private var context : AppContext;		public function MainApplication(){			super();			this.addEventListener(Event.ADDED_TO_STAGE, onAddedToStage_handler);		}		private function onAddedToStage_handler(event : Event) : void {			this.removeEventListener(Event.ADDED_TO_STAGE, onAddedToStage_handler);			context  = new AppContext(this,false);			context.startup();		}			}}